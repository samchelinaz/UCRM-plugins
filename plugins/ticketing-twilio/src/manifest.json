    "displayName": "Ticketing via Twilio",
    "description": "",
    "url": "https://github.com/Ubiquiti-App/UCRM-plugins/tree/master/plugins/sms-twilio",
    "version": "1.0.0",
    "unmsVersionCompliancy": {
      "min": "1.1.0",
      "max": null
    },
